[user]
	name = Matthew Suozzo
[core]
    editor = vim
    excludesfile = ~/.gitignore
    autocrlf = input
[merge]
    tool = vimdiff
    keepBackup = false
[alias]
    ci = commit
    ch = checkout
    cim = commit -m
    st = status
    unstage = reset HEAD --
    clear = checkout --
    last = log -1 HEAD
    visual = !gitk
[push]
    default = current
